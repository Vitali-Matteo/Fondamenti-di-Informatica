#include<stdio.h>

int main(){

  int i, k;

  i = 0;

  k = 0;

  printf(" X ");

  while(k <= 10){

    printf("| %d ", k);

    k = k + 1;

  }

  printf("| \n");

  k = 0;

  while(k < 47){

    printf("_");

    k++;

  }

  k = 0;

  while(i <= 10){

    printf(" %i ", i);

    k = 0;

    while(k <= 10){

      printf("| %d ", i * k);

      k++;

    }

    printf("| \n");

    k = 0;

    while(k < 47){

      printf("_");

      k++;

    }

    printf("\n");

    i++;

  }

  return 0;

}
      

    
