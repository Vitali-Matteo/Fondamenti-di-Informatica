#include<stdio.h>

#define L 10

int main(){

  int v[L];

  int i;

  int k;

  int temp;

  for(i = 0; i < L; i++){

    printf("Inserisci il numero di posizione %d: ", i + 1);

  }

  for(i = 0; i < L; i++){

    for(k = i; k < L; k++){

      if(v[i] > v[k]){

	temp = v[i];

	v[i] = v[k];

	v[k] = temp;

      }

    }

  }

  for(i = 0; i < L; i++){

    printf("Elemnto %d dell' array ordinato Ã¨: %d. \n", i + 1, v[i]);

  }

  return 0;

}

    
