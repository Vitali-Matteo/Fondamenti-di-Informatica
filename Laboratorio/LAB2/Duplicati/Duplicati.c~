#include<stdio.h>

#define N 20

int main(){

  int v[N];

  int Bool[N];

  int i, k;

  int L;

  for(i = 0; i < N; i++){

    printf("Inserisci elemento %d del vettore: ", i + 1);
    scanf("%d", &v[i]);

  }

  for(i = 0; i < N; i++){

    Bool[i] = 1;

  }

  for(i = 0; i < N; i++){

    for(k = i + 1; k < N; k++){

      if(v[i] == v[k]){

	Bool[k] =0;

      }

    }

  }

  printf("Stampo il vettore senza duplicati: \n");

  for(i = 0; i < N; i++){

    if(Bool[i] == 1){

      printf("%d ", V[i]);

    }

  }

  L = 0;

  for(i = 0; i < N; i++){

    if(Bool[i] == 1){

      L++;

    }

  }

  printf("La lunghezza del vettore senza duplicati Ã¨: %d.\n ", L);

  return 0;

}

      

  
